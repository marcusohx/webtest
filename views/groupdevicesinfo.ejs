<% include partials/header2 %>
        <!-- Left navbar-header end -->
        <!-- Page Content -->
        <div id="page-wrapper">
            <div class="container-fluid">
                <div class="row bg-title">
                    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
                        <h4 class="page-title">Group Devices Table</h4> </div>
                    <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12"> <a href="/groupdevices" class="btn btn-danger pull-right m-l-20 btn-rounded btn-outline hidden-xs hidden-sm waves-effect waves-light">Back</a>
                        <ol class="breadcrumb">
                            <li><a href="#">Dashboard</a></li>
                            <li class="active">Group Table</li>
                        </ol>
                    </div>
                    <!-- /.col-lg-12 -->
                </div>
                <!-- /row -->
                <div class="row">
                    <div class="col-sm-12">
                        <div class="white-box">
                            <h3 class="box-title">Group Name - <%=data[0].groupname%></h3>
                            <div class="table-responsive">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th>#</th>
                                            <th>Device ID</th>
                                            <th>Device Name</th>
                                            <th>Group id</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                          <% if(data.length){ 
                                              for(var i = 0;i < data.length;i++) { %>
                                            <tr>
                                              <td><%=(i+1)%></td>
                                              <td><%=data[i].deviceid%></td>
                                              <td><%=data[i].devicename%></td>
                                              <td><%=data[i].groupid%></td>

                                            <% }
                                            }else{ %>
                                            <tr>
                                              <td>No groups</td>
                                            </tr>
                                            <% } %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /.row -->
            </div>
            <!-- /.container-fluid -->
<% include partials/footer2 %>